<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.example.org/air-conditioner" xmlns="http://www.example.org/air-conditioner"
	elementFormDefault="qualified">
	<xs:element name="air-conditioner-data">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="dailyMesurement" type="mesurementType"
					minOccurs="0" maxOccurs="unbounded" />
				<xs:element name="model" type="xs:string" />
				<xs:element name="user-ID" type="xs:string" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="mesurementType">
		<xs:sequence>
			<xs:element name="date" type="xs:date" />
			<xs:element name="measurement" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="temprature">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="value" type="xs:double" />
									<xs:element name="unit" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="time-stump" type="xs:time" />
						<xs:element name="action">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="heater" />
									<xs:enumeration value="cooler" />
									<xs:enumeration value="normal" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>